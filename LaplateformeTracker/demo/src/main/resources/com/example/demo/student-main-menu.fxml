<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controllers.StudentMainMenuController">
   <children>
      <Label layoutX="440.0" layoutY="56.0" prefHeight="95.0" prefWidth="309.0" text="LAPLATEFORME TRACKER">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="showGrades" alignment="CENTER" contentDisplay="CENTER" layoutX="147.0" layoutY="248.0" mnemonicParsing="false" onAction="#handleShowGradesAction" prefHeight="40.0" prefWidth="250.0" text="SHOW GRADES">
         <opaqueInsets>
            <Insets left="50.0" />
         </opaqueInsets>
      </Button>
      <Button fx:id="statistics" layoutX="790.0" layoutY="252.0" mnemonicParsing="false" onAction="#handleStatisticsButtonAction" prefHeight="40.0" prefWidth="250.0" text="STATISTICS" />
      <Button fx:id="changePassword" layoutX="478.0" layoutY="399.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" text="CHANGE PASSWORD" />
      <Button fx:id="logout" layoutX="476.0" layoutY="597.0" mnemonicParsing="false" onAction="#handleBackButtonAction" prefHeight="40.0" prefWidth="250.0" text="LOGOUT" />
      <Label fx:id="studentLabel" layoutX="97.0" layoutY="172.0" text="Student Name">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</Pane>
